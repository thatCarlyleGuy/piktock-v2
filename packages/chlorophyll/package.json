{
  "name": "@cre/chlorophyll",
  "version": "0.1.0",
  "private": true,
  "main": "/dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react-docgen": "^5.4.0",
    "web-vitals": "^2.1.2"
  },
  "resolutions": {
    "webpack": "^5.0.0",
    "webpack-virtual-modules": "^0.4.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "yarn clean && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx src --fix",
    "styleguide": "npx styleguidist server",
    "styleguide:build": "npx styleguidist build",
    "start": "PORT=2020 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@cre/chlorophyll-tailwind": "^0.1.0",
    "@storybook/addon-actions": "^6.5.0-alpha.5",
    "@storybook/addon-essentials": "^6.5.0-alpha.5",
    "@storybook/addon-links": "^6.5.0-alpha.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.0-alpha.5",
    "@storybook/manager-webpack5": "^6.5.0-alpha.5",
    "@storybook/node-logger": "^6.5.0-alpha.5",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.5.0-alpha.5",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-styleguidist": "^11.1.7",
    "tailwindcss": "^3.0.7",
    "webpack": "5"
  }
}
